{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@routes/*": ["routes/*"]
    },
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "declaration": true,
    "outDir": "./dist"
  }
}
